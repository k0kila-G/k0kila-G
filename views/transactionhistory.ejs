     <table class="table mt-2">
         <thead class="bg-primary text-light">
           <tr>
             <th scope="col">Sender</th>
             <th scope="col">Reciver</th>
             <th scope="col">Amount</th>
             <th scope="col">Data</th>
            
             
           </tr>
         </thead>
         <tbody>
           <% if(docs.length>0){%>
             <% docs.forEach((e) => {%>
               <tr>
                 <td><%= e.sender %></td>
                 <td><%= e.receiptner %></td>
                 <td><%= e.amount  %></td>
                 <td><%= e.date  %></td>
                 
               </tr>
                             
               <%}) %>        
             
             <%}else{%>
               <tr class="bg-warning text-light">
                 <td>
                   
                     No records Found!
                   
                 </td>
               </tr>
             
               
               <%} %>
         
         
         </tbody>
       </table>
     </div>
 
     





